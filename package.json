{
  "name": "qiyu-terminal",
  "version": "5.2.1",
  "description": "旗鱼终端 - AI 驱动的跨平台终端",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build:win": "vue-tsc --noEmit && vite build && electron-builder --win",
    "build:mac": "vue-tsc --noEmit && vite build && electron-builder --mac",
    "build:linux": "vue-tsc --noEmit && vite build && electron-builder --linux",
    "build:win:fast": "vite build && electron-builder --win",
    "build:mac:fast": "vite build && electron-builder --mac",
    "build:linux:fast": "vite build && electron-builder --linux",
    "typecheck": "vue-tsc --noEmit",
    "typecheck:electron": "tsc --project tsconfig.node.json --noEmit",
    "typecheck:src": "vue-tsc --noEmit --skipLibCheck",
    "check": "npm run typecheck && npm run lint:check",
    "check:fast": "npm run typecheck:src",
    "build:check": "vue-tsc --noEmit && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx",
    "format": "prettier --write src/",
    "preversion": "git pull --ff-only && git fetch --tags",
    "postversion": "git push && git push origin v$npm_package_version"
  },
  "dependencies": {
    "@lancedb/lancedb": "^0.22.3",
    "@langchain/textsplitters": "^1.0.1",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@xenova/transformers": "^2.17.2",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "apache-arrow": "^18.1.0",
    "electron-store": "^8.2.0",
    "https-proxy-agent": "^7.0.4",
    "jieba-wasm": "^2.4.0",
    "mammoth": "^1.11.0",
    "marked": "^12.0.0",
    "node-pty": "^1.0.0",
    "pdf-parse": "^2.4.5",
    "pdf2json": "^4.0.0",
    "pinia": "^2.1.7",
    "socks-proxy-agent": "^8.0.2",
    "ssh2": "^1.15.0",
    "ssh2-sftp-client": "^12.0.1",
    "strip-ansi": "^7.1.2",
    "uuid": "^9.0.1",
    "vue": "^3.4.21",
    "vue-i18n": "^9.14.5",
    "word-extractor": "^1.0.4"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.2",
    "@rushstack/eslint-patch": "^1.7.2",
    "@types/node": "^20.11.24",
    "@types/ssh2": "^1.15.0",
    "@types/ssh2-sftp-client": "^9.0.6",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^2.0.6"
  },
  "author": "旗鱼",
  "license": "AGPL-3.0"
}
